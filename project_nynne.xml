        }
        sensor_val[side] = fint(uniform);
    }
}

void sRes(direc_t side) {
    if (sensor_on[side]) {
        double uniform = 0.5; //random(1);
        sensor_val[side] = fint(uniform * 30 + 70);
    }
}

