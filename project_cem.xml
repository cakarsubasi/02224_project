		<declaration>// Place local declarations here.
clock x;

int storage = 0;

color_t color;

void sInval(feed_belt_t side) {
    if (sensor_on[side]) {
    double uniform = random(1);
    if (color == YELLOW) {
        uniform = uniform * 10 + 60;
    } else {
        uniform = uniform * 25 + 45;
    }
    sensor_val[side] = fint(uniform);
    }
}

void sVal(feed_belt_t side) {
    if (sensor_on[side]) {
        double uniform = random(1);
        if (color == YELLOW) {
            uniform = uniform * 10 + 50;
        } else {
            uniform = uniform * 15 + 30;
        }
        sensor_val[side] = fint(uniform);
    }
}

void sRes(feed_belt_t side) {
    if (sensor_on[side]) {
        double uniform = random(1);
        sensor_val[side] = fint(uniform * 30 + 70);
    }
}</declaration>
